{% for block in section.blocks %}
  {% case block.type %}
    {% when 'product_info' %}
      {% include "hero_info" %}
    {% when 'product_text' %}
      {% include "panel_text" %}
    {% when 'hero_cta' %}
      {% include "cta_block" %}
    {% when 'product_start' %}
      {% include "start_block" %}
  {% endcase %}
{% endfor %}
