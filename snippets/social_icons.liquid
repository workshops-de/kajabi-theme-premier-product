<style>
  .footer .social-icons__icon,
  .footer .social-icons__icon:hover {
    color: {{ si-TextColor }};
  }
  .footer .social-icons--round .social-icons__icon ,
  .footer .social-icons--square .social-icons__icon {
    background-color: {{ si-BgColor }};
  }
</style>

{% if si-Show %}
  {% if si-newWindow %}
  <div class="social-icons {{ si-Style }} {{ si-Alignment }}">
    <div class="social-icons__row">
      {% if si-Facebook and si-Facebook != "" %}
        <a class="social-icons__icon fa fa-facebook" a target="_blank" href="{{ si-Facebook }}" kjb-settings-id="{{ kjb-Facebook }}"></a>
      {% endif %}
      {% if si-Twitter and si-Twitter != "" %}
        <a class="social-icons__icon fa fa-twitter" a target="_blank" href="{{ si-Twitter }}" kjb-settings-id="{{ kjb-Twitter }}"></a>
