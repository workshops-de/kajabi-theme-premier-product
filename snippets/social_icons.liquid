<!-- Section Variables -->
{% assign si-Show        = section.settings.show_social_icons %}
{% assign si-newWindow   = section.settings.social_icons_new_window %}
{% assign si-TextColor   = section.settings.social_icons_text_color | default: "#ffffff" %}
{% assign si-BgColor     = section.settings.social_icons_background_color | default: settings.color_primary %}
{% assign si-Style       = section.settings.social_icon_background_style %}
{% assign si-Alignment   = section.settings.social_icon_alignment %}
{% assign si-Facebook    = section.settings.social_icon_link_facebook %}
{% assign si-Twitter     = section.settings.social_icon_link_twitter %}
{% assign si-Instagram   = section.settings.social_icon_link_instagram %}
{% assign si-Youtube     = section.settings.social_icon_link_youtube %}
{% assign si-Pinterest   = section.settings.social_icon_link_pinterest %}
{% assign si-Google      = section.settings.social_icon_link_google_plus %}
{% assign si-Vimeo       = section.settings.social_icon_link_vimeo %}
{% assign si-Github      = section.settings.social_icon_link_github %}
{% assign si-Apple       = section.settings.social_icon_link_itunes %}
{% assign si-Linkedin    = section.settings.social_icon_link_linkedin %}
{% assign si-Soundcloud  = section.settings.social_icon_link_soundcloud %}
{% assign si-Yelp        = section.settings.social_icon_link_yelp %}
{% assign si-Tumblr      = section.settings.social_icon_link_tumblr %}
{% assign si-Slack       = section.settings.social_icon_link_slack %}
{% assign si-Flickr      = section.settings.social_icon_link_flickr %}
{% assign si-Dribbble    = section.settings.social_icon_link_dribbble %}

<!-- KJB Settings Variables -->
{% assign kjb-Facebook   = 'social_icon_link_facebook' | settings_id: section: section %}
{% assign kjb-Twitter    = 'social_icon_link_twitter' | settings_id: section: section %}
{% assign kjb-Instagram  = 'social_icon_link_instagram' | settings_id: section: section %}
{% assign kjb-Youtube    = 'social_icon_link_youtube' | settings_id: section: section %}
{% assign kjb-Pinterest  = 'social_icon_link_pinterest' | settings_id: section: section %}
{% assign kjb-Google     = 'social_icon_link_google_plus' | settings_id: section: section %}
{% assign kjb-Vimeo      = 'social_icon_link_vimeo' | settings_id: section: section %}
{% assign kjb-Github     = 'social_icon_link_github' | settings_id: section: section %}
{% assign kjb-Apple      = 'social_icon_link_itunes' | settings_id: section: section %}
